{
  "name": "@orion/demo-app",
  "version": "1.0.0",
  "description": "Deploy a demo GraphQL app to AWS Lambda for testing Orion edge cache",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:lambda && tsc",
    "build:lambda": "cd lambda-src && npm install && npm run build",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist && cd lambda-src && npm run clean"
  },
  "keywords": [
    "orion",
    "graphql",
    "demo",
    "aws",
    "lambda",
    "deploy"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.400.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src",
    "assets",
    "terraform",
    "lambda-src",
    "tsconfig.json"
  ]
}
